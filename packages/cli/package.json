{
  "name": "@ai-ext/cli",
  "version": "0.1.0",
  "description": "The ai-ext CLI â€” init, build, validate, serve",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ai-ext": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target node && bun run generate-declarations",
    "generate-declarations": "tsc --emitDeclarationOnly",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "files": ["dist"],
  "dependencies": {
    "@ai-ext/schema": "workspace:*",
    "@ai-ext/core": "workspace:*",
    "@ai-ext/compiler": "workspace:*",
    "@ai-ext/runtime": "workspace:*",
    "commander": "^13.0.0",
    "chalk": "^5.4.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.7.0"
  }
}
